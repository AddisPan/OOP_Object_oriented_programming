import java.util.Scanner;

public class LockerDoors {
public static void main(String[] args) {
	Scanner input = new Scanner(System.in);
	System.out.println("Please number¡G");
	int n = input.nextInt();
	int j;
	for(int i=1;i<n;i++) {
		j = i;
		while(j <= n) {
			String[] A = new String[n];
			if (A[j]=="0") {
				A[j] = "1";
			}
			else
				A[j] = "0";
			j = j + 1;
		}
	}
}
}
