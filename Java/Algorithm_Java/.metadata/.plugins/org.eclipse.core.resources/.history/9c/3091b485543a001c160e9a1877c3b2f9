import java.util.Scanner;

public class Qn {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);

		System.out.print("num1:");
		int n = input.nextInt();
		System.out.print(f(n));
	}

	public static int f(int n) {
		if (n == 0)
			return 1;
		return f(n - 1) * n;
	}
}
