import java.util.Scanner;

public class One {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);

		System.out.print("num1:");
		int n = input.nextInt();
		System.out.print(One(n));
	}

	public static int One(int n) {
		if (n == 1)
			return 1;
		return 2 * One(n-3);
	}
}
