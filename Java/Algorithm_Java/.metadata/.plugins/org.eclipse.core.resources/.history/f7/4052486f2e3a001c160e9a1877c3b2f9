
public class Fib {

	public static void main(String[] args) {
		
	}
	public static int fib(int n) {
		int a=1, b=1, c;
		System.out.print(a + " " + b);
		
		for (int i = 2; i < n; i++)
		{
			c = a + b; 				   // the third result = the second result + the first result
			System.out.print(" " + c); // print result
			a = b; 					   // a move next b
			b = c; 					   // b move next c
		}
	}
}
