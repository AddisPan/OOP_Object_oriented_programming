public class  Memory{
		public int x=1;
		private int u=4;
		protected void fun(int z){
			int y;
			y=50+z;
			System.out.println("Memory函數的區域變數y="+y);	
		}
		public static void main(String[] args){
	 		Memory b=new Memory();
	 		System.out.println("Memory成員資料="+b.x);
			b.fun(5);
			b.fun(5);
			b.x=b.x+1;
			System.out.println("Memory成員資料="+b.x);
	}	
}

